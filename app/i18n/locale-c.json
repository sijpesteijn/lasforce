                "laser": "/lasforce/rest"
            },
            "templ": {
                "laser": "ws://localhost:7070/lasforce/ws/laser",

                "animation-list": "$t(settings.rest.base.laser)/animations/:id",
                "animation-update": "$t(settings.rest.base.laser)/animations/:animation",
